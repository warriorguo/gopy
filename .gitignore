*.sublime-project
*.sublime-workspace

# Example output files (keep examples but not their compiled output)
# ------------------------------------------------------------------
examples/*.pyc
examples/*.pybc

# Test scenario outputs
# ---------------------
scenarios/output/
test_scenarios/

# Performance test artifacts
# --------------------------
perf.data*
*.trace

# Go module cache (usually handled globally but can be local)
# ----------------------------------------------------------
go.sum.bak

# Local development overrides
# ---------------------------
local.mk
Makefile.local
.env.development

# Security files (never commit)
# ------------------------------
*.pem
*.key
*.crt
*.p12
secrets.txt
passwords.txt

# Compressed archives
# -------------------
*.zip
*.tar.gz
*.tgz
*.rar
*.7z

# Node.js (if any web components are added later)
# -----------------------------------------------
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Python (if any Python scripts are used for testing)
# ---------------------------------------------------
__pycache__/
*.py[cod]
*$py.class
*.so
.Python
env/
venv/
ENV/

